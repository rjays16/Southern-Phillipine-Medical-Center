a#return_button {
  background: #F4F1EC url(../../../images/or_main_images/return_button.png) no-repeat;
  background-position: 0px 0px;
  width: 75px;
  height: 25px;
  border: none;
  outline: none;
  cursor: pointer;
  display: block;
  
}

a#return_button:hover {
  background-position: 0px -25px;
  outline: none;
  cursor: pointer;
}

