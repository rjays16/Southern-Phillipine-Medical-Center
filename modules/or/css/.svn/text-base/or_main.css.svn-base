  height: 25px;
  border: none;
  outline: none;
  cursor: pointer;
  float: left;
  margin-right: 10px;
}

div#approve_or_main_request div.body form input#or_approve_submit:hover,
div#disapprove_or_main_request div.body form input#or_disapprove_submit:hover, 
