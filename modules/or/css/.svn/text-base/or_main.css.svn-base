


/** For the OR Main Request
  * Added by Omick, December 20, 2008
  */
#or_main_request {
  padding: 0px 5px 5px 10px;
  text-align: left;
}

#or_main_request form, #or_main_request input, #or_main_request_select, #or_main_request table, #or_main_request tr, #or_main_request td, #or_main_request div, 
#or_main_request span {
  padding: 0px;
  margin: 0px;
}

#or_main_request fieldset {
  padding: 0px 15px 15px 15px;
  margin: 0 0 20px;
  background: #F4F1EC url(../../../images/or_main_images/fieldset_back.jpg) repeat-x;
  border: 1px solid #D6D3CC;
}

#or_main_request legend {
  color: #FFFFFF;
  background-color: #4899BE;
  font-family: Arial, Helvetica, Sans-serif;
  font-size: 0.9em;
  font-weight: bold;
  padding: 5px;
  margin-bottom: 0;
  width: 9em;
  border: 1px solid #3E4A49;
  cursor: pointer;
}

#or_main_request span#reminder {
  font: normal 11px Arial;
  color: #292929;
  line-height: 3em;
}

#or_main_request label {
  color: #4C4B4A;
  
  display:inline-block;
}

#or_main_request input[type=text] {
  border: 1px #CAC9B9 solid;
  height: 20px;
  padding: 2px;
  font: normal 11px Arial;
  width: 180px;  
  display:inline-block;
}

#or_main_request input[type=text].error_field {
  border: 1px #FF0000 solid;
}

#or_main_request span#error_form_input {
  color:red;
  font-size:0.8em;
  margin-left:5px;
}

#or_main_request tr {
  vertical-align:  bottom;
}

#or_main_request input[type=radio] {
  margin-top: 8px;
}

#or_main_request a div#select_patient_button {
  background:  url(../../../images/or_main_images/select_patient.png);
  width: 105px;
  height: 31px;
  float: left;
  opacity: 0.7;
}

#or_main_request a div#select_patient_button:hover, #or_main_request img.date_time_picker:hover {
  opacity: 1;
}

#or_main_request img.date_time_picker {
  background:  url(../../../images/or_main_images/date_time_picker.png);
  width: 105px;
  height: 31px;
  opacity: 0.7;
  cursor: pointer;
}

#or_main_request textarea {
  width: 180px;
  height: 50px;
  border: 1px #CAC9B9 solid;
  padding: 3px;
  font: normal 11px Arial;
}

#or_main_request input#or_main_submit {
  background: #F4F1EC url(../../../images/or_main_images/submit_request.png) no-repeat;
  background-position: 0px 0px;
  width: 75px;
  height: 25px;
  border: none;
  outline: none;
  cursor: pointer;
  float: left;
  margin-right: 10px;
}

#or_main_request input#or_main_submit:hover, #or_main_request a#request_cancel:hover {
  background-position: 0px -25px;
  outline: none;
  cursor: pointer;
}

#or_main_request a#request_cancel {
  background: #F4F1EC url(../../../images/or_select_images/charge_cancel_button.png) no-repeat;
  background-position: 0px 0px;
  width: 75px;
  height: 25px;
  border: none;
  outline: none;
  cursor: pointer;
  display: block;
  float: left;
}

#or_main_request div.date_display {
  display:inline-block;
  border: 1px #CAC9B9 solid;
  height: 14px;
  padding: 2px;
  font: normal 11px Arial;
  width: 175px;
  background: #FFFFFF; 
  color: #000000;    
}

#or_main_request input[type=radio] {
  background: #FF0000;
  border: 1px #FF0000 solid;
}

#or_main_request div.pseudo_input {
  display:inline-block;
  border: 1px #CAC9B9 solid;
  height: 14px;
  padding: 2px;
  font: normal 11px Arial;
  width: 175px;
  background: #FFFFFF; 
  color: #000000;
}


